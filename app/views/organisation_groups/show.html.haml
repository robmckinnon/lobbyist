- @title = @group.name
#organisation_group_members
  - unless @organisations.empty?
    %h2 Organisations in group
    %ul
      - @organisations.each do |organisation|
        %li= link_to organisation.name, organisation_path(organisation)
#members_interests_items
  = render :partial => 'organisations/members_interests_items', :object => @members_interests_items
#appointments
  = render :partial => 'organisations/appointments', :object => @appointments
#lobbying_clients
  = render :partial => 'organisations/lobbying_clients', :locals => {:consultancy_client_organisations => @consultancy_client_organisations, :consultancy_entries_by_client_organisation => @consultancy_entries_by_client_organisation, :monitoring_client_organisations => @monitoring_client_organisations, :monitoring_entries_by_client_organisation => @monitoring_entries_by_client_organisation, :partial_path => 'organisations/' }
#lobbyists_retained
  = render :partial => 'organisations/lobbyists_retained', :locals => {:partial_path => 'organisations/'}
#lobbyist_staff
  = render :partial => 'organisations/lobbyists_staff', :locals => {:partial_path => 'organisations/'}

